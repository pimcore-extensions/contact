<?xml version="1.0"?>
<zend-config xmlns:zf="http://framework.zend.com/xml/zend-config-xml/1.0/">
    <plugin>
        <pluginName>PimContact</pluginName>
        <pluginDescription>
            Contact form with history.
        </pluginDescription>
        <pluginNiceName>Pimcore Contact Plugin</pluginNiceName>
        <pluginIcon>/plugins/PimContact/static/img/contact_logo.png</pluginIcon>
        <pluginServer>plugins.pimcore.org</pluginServer>
        <pluginVersion>1.0.0</pluginVersion>
        <pluginRevision></pluginRevision>
        <pluginBuildTimestamp></pluginBuildTimestamp>
        <pluginClassName>PimContact_Plugin</pluginClassName>
        <pluginIncludePaths>
            <path>/PimContact/lib</path>
            <path>/PimContact/controllers</path>
            <path>/PimContact/models</path>
        </pluginIncludePaths>
        <pluginNamespaces>
            <namespace>Modern</namespace>
            <namespace>PimContact</namespace>
        </pluginNamespaces>
        <pluginJsPaths>
            <path>/PimContact/static/js/plugin.js</path>
            <path>/PimContact/static/js/history.js</path>
            <path>/PimContact/static/js/rowexpander.js</path>
